<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:map="clr-namespace:OwnersSimulation.View.CustomControls.Map"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Style TargetType="{x:Type map:MapBlockControl}" x:Key="MapBlockControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type map:MapBlockControl}">
                    <Border Width="19" Height="19" Margin="1" Background="AliceBlue" BorderThickness="0.2" BorderBrush="Aqua">
                        
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
</ResourceDictionary>